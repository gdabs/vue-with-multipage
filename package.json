{
  "name": "vue-ssr-with-multipage",
  "version": "1.0.0",
  "private": true,
  "types": "./src/typings/*.d.ts",
  "scripts": {
    "start": "ts-node build/index.ts start",
    "build:test": "rimraf dist && ts-node build/index.ts build --test",
    "build:production": "rimraf dist && ts-node build/index.ts build --production",
    "analyze": "cross-env GENERATE_ANALYSIS=true npm run build",
    "lint": "npm run lint:js && npm run lint:style && npm run lint:prettier",
    "lint-staged": "lint-staged",
    "lint-staged:js": "eslint --ext .js,.jsx,.ts,.tsx,.vue --ignore-path .eslintignore .",
    "lint:js": "eslint --fix --cache --ext .js,.jsx,.ts,.tsx,.vue --format=stylish ./src --ignore-path .eslintignore .",
    "lint:prettier": "check-prettier lint ./src --ignore-path .eslintignore .",
    "lint:style": "stylelint --fix \"src/**/*.less\" --syntax less",
    "prettier": "prettier -c --write \"src/**/*\""
  },
  "dependencies": {
    "axios": "^0.21.1",
    "js-cookie": "^2.2.1",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "vue": "^3.0.0",
    "vuex": "^4.0.0",
    "webpack-dev-server-ssr": "^3.10.3"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.9.6",
    "@babel/preset-env": "^7.5.5",
    "@babel/preset-typescript": "^7.12.13",
    "@types/js-cookie": "^2.2.6",
    "@types/webpack": "^4.41.29",
    "@types/workbox-webpack-plugin": "^5.1.7",
    "@types/yargs": "^13.0.4",
    "@typescript-eslint/eslint-plugin": "^4.27.0",
    "@typescript-eslint/parser": "^4.27.0",
    "@vue/babel-plugin-jsx": "^1.0.3",
    "@vue/compiler-sfc": "^3.0.7",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-loader": "8.0.4",
    "babel-plugin-import": "^1.13.0",
    "check-prettier": "^1.0.3",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.3",
    "css-hot-loader": "^1.4.4",
    "css-loader": "1.0.0",
    "css-modules-require-hook": "^4.2.3",
    "copy-webpack-plugin": "4.6.0",
    "dotenv": "^10.0.0",
    "dotenv-expand": "^5.1.0",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.0.0",
    "file-loader": "2.0.0",
    "html-webpack-plugin": "^4.4.1",
    "husky": "^6.0.0",
    "ignore-loader": "^0.1.2",
    "less": "^4.1.1",
    "less-loader": "^7.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mocker-api": "^2.9.0",
    "mockjs": "^1.1.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "ora": "^5.4.1",
    "postcss-assets": "5.0.0",
    "postcss-color-rgba-fallback": "^4.0.0",
    "postcss-discard-comments": "^4.0.2",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-loader": "3.0.0",
    "postcss-opacity": "^5.0.0",
    "postcss-preset-env": "^6.0.5",
    "postcss-px2rem-exclude": "^0.0.6",
    "postcss-safe-parser": "4.0.1",
    "prettier": "^2.2.1",
    "promisify": "^0.0.3",
    "rimraf": "^3.0.2",
    "shelljs": "^0.8.4",
    "stylelint": "^13.8.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-standard": "^20.0.0",
    "terser-webpack-plugin": "^2.3.5",
    "ts-node": "^10.0.0",
    "typescript": "^3.9.7",
    "url-loader": "1.1.1",
    "vue-loader": "^16.1.2",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-chain": "^6.4.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpackbar": "^5.0.0-3",
    "workbox-webpack-plugin": "^6.1.5",
    "yargs": "^13.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged",
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.less": "stylelint --syntax less",
    "**/*.{js,jsx,tsx,ts,vue,less,md,json}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{jsx}": "npm run lint-staged:js",
    "**/*.{ts,tsx,vue}": "npm run lint-staged:js"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
